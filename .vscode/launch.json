    "version": "0.2.0",
    "configurations": [
        {
            "name": "Explorer",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "console": "integratedTerminal",
            "program": "${workspaceFolder}/cmd/explorer/main.go",
            "args": [
                "--config=${workspaceFolder}/config/sos_explorer.yaml"
            ],
            "env": {
                "ELASTIC_APM_SERVICE_NAME": "sos-explorer",
                "ELASTIC_APM_SERVER_URL": "http://localhost:8200"
            }
        },
